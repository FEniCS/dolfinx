from ufl import *

ORDER = 1

domain = triangle

# Define element and FE solution
P1 = FiniteElement("Lagrange", domain, ORDER)
uh = Coefficient(P1)

# Exact solution
P3 = FiniteElement("Lagrange", domain, ORDER + 2)
u = Coefficient(P3)

# Squared L2 norm of error
M = inner(grad(u - uh), grad(u - uh))*dX
