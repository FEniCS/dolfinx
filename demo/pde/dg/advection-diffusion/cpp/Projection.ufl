DG = FiniteElement("Discontinuous Lagrange", "triangle", 1)
CG = FiniteElement("Lagrange", "triangle", 1)

# Test and trial functions
v   = TestFunction(CG)
phi = TrialFunction(CG)

phi0 = Coefficient(DG)

a = dot(v, phi)*dx
L = dot(v, phi0)*dx

